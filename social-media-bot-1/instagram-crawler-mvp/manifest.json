{
  "manifest_version": 3,
  "name": "Instagram Saved + Friends Organizer (MVP)",
  "description": "인스타그램 저장글/팔로우 목록을 크롤링해 카테고리로 정리하는 크롬 확장 MVP",
  "version": "0.1.0",
  "key": "",
  "action": {
    "default_title": "IG Organizer",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": [
    "https://*.instagram.com/*",
    "https://www.instagram.com/*",
    "https://instagram.com/*",
    "https://m.instagram.com/*"
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*.instagram.com/*",
        "https://www.instagram.com/*",
        "https://instagram.com/*",
        "https://m.instagram.com/*"
      ],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
